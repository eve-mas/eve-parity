   :: s0 and ((ca and !cb) or (!ca and cb)) ~> s0' := false, s1' := true, s2':=false , s3':=false, s4':=false, s5':=false , s6':=false, s7':=false, p' := false, q' := false;
   :: s0 and (ca <-> cb) ~> s0' := false, s1' := false, s2' := true, s3' := false, s4' := false, s5':=false, s6':=false, s7':=false, p' := false, q' := false;
   :: s1 and ((ca and (!c1 and !c0)) or (!ca and (c1 and !c0))) ~> s0' := false, s1' := false,s2':=false, s3':=true, s4':=false, s5':=false, s6':=false, s7':=false, p' := true, q' := false;
   :: s1 and ((cb and (!c1 and c0)) or (!cb and (c1 and c0))) ~> s0' := false, s1' := false, s2':=false, s3':=false, s4':=true, s5':=false, s6':=false, s7':=false, p' := false, q' := true;
   :: s1 and ((!ca and (!c1 and !c0)) or (ca and (c1 and !c0)) or (!cb and (!c1 and c0) or (cb and (c1 and c0)))) ~> s0' := false, s1' := false,s2':=true, s3':=false, s4':=false, s5':=false, s6':=false, s7':=false, p' := false, q' := false;
   :: s2 ~> s0' := false, s1' := false,s2':=false, s3':=false, s4':=false, s5':=true, s6':=false, s7':=false, p' := false, q' := false;
   :: s4 and ((ca and (!c1 and !c0)) or (!ca and (c1 and !c0))) ~> s0' := false, s1' := false,s2':=false, s3':=false, s4':=false, s5':=false, s6':=true, s7':=false, p' := true, q' := false;
   :: s4 and ((cb and (!c1 and c0)) or (!cb and (c1 and c0))) ~> s0' := false, s1' := false, s2':=false, s3':=false, s4':=false, s5':=false, s6':=false, s7':=true, p' := false, q' := true;
   :: s4 and ((!ca and (!c1 and !c0)) or (ca and (c1 and !c0)) or (!cb and (!c1 and c0) or (cb and (c1 and c0)))) ~> s0' := false, s1' := false,s2':=false, s3':=false, s4':=false, s5':=true, s6':=false, s7':=false, p' := false, q' := false;
   :: s5 ~> s0' := false, s1' := false, s2':=false, s3':=false, s4':=false, s5':=true, s6':=false, s7':=false, p' := false, q' := false;
   :: s6 ~> s0' := false, s1' := false, s2':=false, s3':=false, s4':=false, s5':=false, s6':=true, s7':=false, p' := true, q' := false;
   :: s7 ~> s0' := false, s1' := false, s2':=false, s3':=false, s4':=false, s5':=false, s6':=false, s7':=true, p' := false, q' := true;
